@{
}
@model Subcategory


<form asp-controller="Admin" asp-action="AddSubCategory"  method="post">
   <label asp-for="Name"></label>
   <input asp-for="Name" type="Text"></input>
   <button type="submit">Add</button>
</form>

<ul>
    @foreach(var subCategory in ViewBag.SubCategories)
    {
        <li>
             <a href="/Admin/AddProductType/@subCategory.SubcategoryId">@subCategory.Name</a>
        </li>
    }

</ul>