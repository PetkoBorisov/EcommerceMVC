@{
}
@model ProductType


<form asp-controller="Admin" asp-action="AddProductType"  method="post">
   <label asp-for="Name"></label>
   <input asp-for="Name" type="Text"></input>
   <button type="submit">Add</button>
</form>

<ul>
    @foreach(var productType in ViewBag.ProductTypes)
    {
        <li>
             <a href="#">@productType.Name</a>
        </li>
    }

</ul>